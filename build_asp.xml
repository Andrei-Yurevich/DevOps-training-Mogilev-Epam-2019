<project>
<actions/>
<description>Build asp .net core app and trigger octopus.</description>
<keepDependencies>false</keepDependencies>
<properties>
<hudson.model.ParametersDefinitionProperty>
<parameterDefinitions>
<hudson.model.StringParameterDefinition>
<name>release_version</name>
<description/>
<defaultValue>0.0.0</defaultValue>
<trim>true</trim>
</hudson.model.StringParameterDefinition>
</parameterDefinitions>
</hudson.model.ParametersDefinitionProperty>
</properties>
<scm class="hudson.plugins.git.GitSCM" plugin="git@3.9.3">
<configVersion>2</configVersion>
<userRemoteConfigs>
<hudson.plugins.git.UserRemoteConfig>
<name>Epam DevOps training</name>
<url>
https://github.com/Andrey1913/DevOps-training-Mogilev-Epam-2019
</url>
</hudson.plugins.git.UserRemoteConfig>
</userRemoteConfigs>
<branches>
<hudson.plugins.git.BranchSpec>
<name>module5</name>
</hudson.plugins.git.BranchSpec>
</branches>
<doGenerateSubmoduleConfigurations>false</doGenerateSubmoduleConfigurations>
<submoduleCfg class="list"/>
<extensions/>
</scm>
<canRoam>true</canRoam>
<disabled>false</disabled>
<blockBuildWhenDownstreamBuilding>true</blockBuildWhenDownstreamBuilding>
<blockBuildWhenUpstreamBuilding>true</blockBuildWhenUpstreamBuilding>
<triggers/>
<concurrentBuild>false</concurrentBuild>
<builders>
<hudson.plugins.powershell.PowerShell plugin="powershell@1.3">
<command>
dotnet publish --framework netcoreapp2.0 git status
</command>
</hudson.plugins.powershell.PowerShell>
</builders>
<publishers>
<hudson.plugins.octopusdeploy.OctopusDeployReleaseRecorder plugin="octopusdeploy@1.9.0">
<serverId>46316-97776-64426-02372</serverId>
<project>asp_music</project>
<environment/>
<tenant/>
<waitForDeployment>false</waitForDeployment>
<releaseVersion>${release_version}</releaseVersion>
<releaseNotes>false</releaseNotes>
<channel>Default</channel>
<releaseNotesJenkinsLinkback>false</releaseNotesJenkinsLinkback>
<releaseNotesFile/>
<deployThisRelease>false</deployThisRelease>
<defaultPackageVersion/>
</hudson.plugins.octopusdeploy.OctopusDeployReleaseRecorder>
</publishers>
<buildWrappers/>
</project>